#include<bits/stdc++.h>

using namespace std;

int main()
{
    int t,a,b,c;
    scanf("%d",&t);
    for(int i=1;i<=t;i++){
        int ans;
        scanf("%d%d%d",&a,&b,&c);
        if((a>b&&a<c)||(a>c&&a<b)) ans=a;
       else if((b>a&&b<c)||(b>c&&b<a)) ans=b;
       else if((c>b&&c<a)||(c>a&&c<b)) ans=c;
        printf("Case %d: %d\n",i,ans);
    }
    return 0;
}
